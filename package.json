{
  "name": "markdown-player",
  "version": "0.1.0",
  "description": "A markdown text-to-speech player",
  "main": "dist/markdown-player.js",
  "scripts": {
    "build": "./node_modules/.bin/standard lib/*.js && WEBPACK_ENV=build ./node_modules/.bin/webpack -p --progress",
    "dev": "WEBPACK_ENV=dev ./node_modules/.bin/webpack --progress --colors --watch",
    "lint": "./node_modules/.bin/standard lib/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderchang/markdown-player.git"
  },
  "keywords": [
    "markdown",
    "text-to-speech",
    "video",
    "player"
  ],
  "author": "Wonder Chang <iwonder.tw@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wonderchang/markdown-player/issues"
  },
  "homepage": "https://github.com/wonderchang/markdown-player#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.28.0",
    "standard": "^10.0.1",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "semantic-ui-css": "^2.2.10"
  },
  "standard": {
    "globals": [
      "history",
      "location",
      "SpeechSynthesisUtterance"
    ]
  }
}
