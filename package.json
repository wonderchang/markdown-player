{
  "name": "markdown-player",
  "version": "0.1.0",
  "description": "A markdown text-to-speech player",
  "main": "dist/markdown-player.js",
  "scripts": {
    "build": "./node_modules/.bin/standard lib/*.js && ./node_modules/.bin/webpack --config webpack.config.prod.babel.js --progress",
    "dev": "./node_modules/.bin/webpack-dev-server --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderchang/markdown-player.git"
  },
  "keywords": [
    "markdown",
    "text-to-speech",
    "video",
    "player"
  ],
  "author": "Wonder Chang <iwonder.tw@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wonderchang/markdown-player/issues"
  },
  "homepage": "https://github.com/wonderchang/markdown-player#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "html-webpack-plugin": "^2.28.0",
    "standard": "^10.0.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "marked": "^0.3.6"
  },
  "standard": {
    "globals": [
      "history",
      "location",
      "SpeechSynthesisUtterance"
    ]
  }
}
